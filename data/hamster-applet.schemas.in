<gconfschemafile>
    <schemalist>
        <schema>
            <key>/schemas/apps/hamster-applet/general/keybinding</key>
            <applyto>/apps/hamster-applet/general/keybinding</applyto>
            <owner>hamster-applet</owner>
            <type>string</type>
            <default>&lt;Super&gt;H</default>
            <locale name="C">
                <short>Show window</short>
                <long>Keyboard shortcut for showing the Hamster Applet window.</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/hamster-applet/general/timeout</key>
            <applyto>/apps/hamster-applet/general/timeout</applyto>
            <owner>hamster-applet</owner>
            <type>int</type>
            <default>30</default>
            <locale name="C">
                <short>Timeout in minutes</short>
                <long>
                    After how many minutes hamster should stop tracking activity
                    when computer is idle. Set to 0 to disable.
                </long>
            </locale>
        </schema>
    </schemalist>
</gconfschemafile>
